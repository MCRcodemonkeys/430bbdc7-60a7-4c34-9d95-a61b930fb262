              "PathCA":"ca_cert.pem",
              "PathCRT":"crt_cert.pem",
              "ExpiryDate":"20/2/2022"
              }
      },
      {
          "Plugin": "Beats-SSL",
          "Port": 11435,
          "Protocol": "tcp",
