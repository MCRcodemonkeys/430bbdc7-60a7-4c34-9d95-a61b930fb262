              "Path": "example.com.pem",
              "ExpiryDate":"20/2/21"
              }
      },
      {
          "Plugin": "TCP",
          "Port": 11436,
          "Protocol": "tcp",
          "Codec": "json"
      },
      {
          "Name" : "prod-firewall",
          "Plugin": "TCP-SSL",
          "Port": 11437,
          "Protocol": "tcp",
          "Codec": "json",
