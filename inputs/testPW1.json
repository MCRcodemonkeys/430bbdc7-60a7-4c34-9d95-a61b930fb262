{
  "LogstashInputs": [
      {
          "Plugin": "Syslog-TEST1",
          "Port": 11432,
          "Protocol": "tcp"
      },
      {
          "Plugin": "Syslog",
          "Port": 11432,
          "Protocol": "udp"
      },
      {
          "Name": "stupid old router",
          "Plugin": "Syslog-SSL",
          "Port": 11433,
          "Protocol": "tcp",
          "TLS_VERSION": "TLS1.0"
      },
      {
          "Name" : "dev-mtls",
